{
  "name": "@statewalker/fsm-process",
  "version": "0.17.2",
  "description": "FSM-based processes",
  "keywords": [],
  "homepage": "https://github.com/statewalker/statewalker-fsm-process",
  "author": {
    "name": "Mikhail Kotelnikov",
    "email": "mikhail.kotelnikov@gmail.com"
  },
  "license": "MIT",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "source": "./src/index.ts",
  "module": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./*": {
      "source": "./src/*.ts",
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "default": "./dist/*.js"
    }
  },
  "dependencies": {
    "@statewalker/fsm": "workspace:*",
    "@statewalker/fsm-validator": "workspace:*"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.0",
    "tsdown": "^0.12.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:statewalker/statewalker-fsm-process.git"
  },
  "scripts": {
    "dist": "pnpm test && tsdown",
    "build": "pnpm dist",
    "watch": "tsdown --watch",
    "clean": "rm -rf dist",
    "format": "biome format . --write",
    "check": "biome check .",
    "fix": "pnpm format && biome check . --fix --unsafe",
    "test": "vitest --run",
    "test:watch": "vitest",
    "prepublish": "pnpm dist"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  }
}
